<%- include('./partials/nav') %>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Menu</title>
</head>
<body>
    <h1>Full Menu</h1>
     <ul>
        <% menu.forEach((item,index) => { %>
                 <li>Name: <%= item.name%> |
                    Price: <%= item.price%> |
                    Rating: <%= item.rating%> | 
                    Details: <%= item.details%>
                </li>
        <% }); %>
    </ul>
           
</body>
</html>